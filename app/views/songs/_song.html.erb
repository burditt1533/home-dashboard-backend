<div id="<%= dom_id song %>">
  <p>
    <strong>Name:</strong>
    <%= song.name %>
  </p>

  <p>
    <strong>Artist:</strong>
    <%= song.artist %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= song.url %>
  </p>

  <p>
    <strong>Tempo:</strong>
    <%= song.tempo %>
  </p>

  <p>
    <strong>Integer:</strong>
    <%= song.integer %>
  </p>

  <p>
    <strong>Lyrics:</strong>
    <%= song.lyrics %>
  </p>

  <p>
    <strong>Jsonb:</strong>
    <%= song.jsonb %>
  </p>

</div>
